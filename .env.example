# Kasparro AI - Environment Configuration
# Copy this file to .env and fill in your values

# Google AI Studio API Key (Required)
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model (Optional - defaults to gemini-2.5-flash)
# Options: gemini-2.0-flash, gemini-2.5-flash, gemini-2.5-pro
GEMINI_MODEL=gemini-2.5-flash

# Output Directory (Optional - defaults to outputs/)
OUTPUT_DIR=outputs

# Enable Debug Logging (Optional - defaults to False)
DEBUG=False
